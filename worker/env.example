# Environment Configuration for XspensesAI Worker

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_PUBLIC_ANON_KEY=your_supabase_anon_key

# Storage Buckets
SUPABASE_BUCKET_ORIGINALS=original_docs
SUPABASE_BUCKET_REDACTED=redacted_docs

# Redis Configuration
REDIS_URL=redis://localhost:6379

# OCR Configuration
OCR_ENGINE=ocrspace
OCRSPACE_API_KEY=your_ocr_space_api_key

# Optional: Google Vision (fallback)
GOOGLE_PROJECT_ID=your_google_project_id
GOOGLE_CREDENTIALS_JSON=your_google_credentials_json

# AI Configuration
USE_LLM_FALLBACK=false
OPENAI_API_KEY=your_openai_api_key

# Worker Configuration
PORT=8080
WORKER_CONCURRENCY=5
LOG_LEVEL=info

# Security
DELETE_ORIGINAL_ON_SUCCESS=true

# Health Check
HEALTH_CHECK_INTERVAL=30000



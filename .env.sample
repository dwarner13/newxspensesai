# Supabase Configuration
# Required for transaction persistence, memory storage, and XP tracking
# Without these, OCR will work but transactions won't be saved to database

SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# OpenAI Configuration
# Required for chat completions, embeddings, and moderation

OPENAI_API_KEY=

# Optional: OCR Providers
# OCR will work without these (returns warnings), but with better results if configured

OCRSPACE_API_KEY=
GOOGLE_APPLICATION_CREDENTIALS=

# Optional: Feature Flags
# Defaults shown below (all enabled by default)

ENABLE_GUARDRAILS=true
ENABLE_PII_MASKING=true
ENABLE_MEMORY=true
ENABLE_XP=true

  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8888 --host",
    "build": "tsc && vite build",
    "build:ci": "pnpm -s build",
    "predeploy:ci": "pnpm -s build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "vite": "vite",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "parity": "tsx scripts/parityTest.ts --old=http://localhost:3000/old-chat --new=http://localhost:8888/.netlify/functions/chat",
    "parity:custom": "tsx scripts/parityTest.ts",
    "test:delegate": "tsx scripts/localDelegateCheck.ts",
    "sync:crystal": "ts-node scripts/sync-employee-prompt.ts crystal-analytics ./docs/CRYSTAL_2.0_SYSTEM_PROMPT.md",
    "sync:prime": "ts-node scripts/sync-employee-prompt.ts prime-boss ./docs/PRIME_CEO_PERSONA.md",
    "sync:byte": "ts-node scripts/sync-employee-prompt.ts byte-docs ./docs/BYTE_PERSONA.md",
    "sync:tag": "ts-node scripts/sync-employee-prompt.ts tag-categorizer ./docs/TAG_PERSONA.md",
    "sync:ledger": "ts-node scripts/sync-employee-prompt.ts ledger-tax ./docs/LEDGER_PERSONA.md",
    "sync:goalie": "ts-node scripts/sync-employee-prompt.ts goalie-agent ./docs/GOALIE_PERSONA.md",
    "sync:all": "npm run sync:crystal && npm run sync:prime && npm run sync:byte && npm run sync:tag && npm run sync:ledger && npm run sync:goalie && echo 'âœ… All employees synced!'",
    "memcap": "tsx scripts/memory_cap_scan.ts",
    "memcap:live": "tsx scripts/memory_cap_scan.ts --live",
    "test:crystal": "tsx scripts/test-crystal.ts",
    "test:registry": "tsx scripts/test-employee-registry.ts",
    "test:finley": "tsx scripts/test-finley.ts",
    "test:finley-routing": "tsx scripts/test-finley-routing.ts",
    "netlify:dev": "netlify dev --port 8888",
    "worker:dev": "tsx watch worker/src/index.ts",
    "functions:build": "tsx scripts/build-functions.ts",
    "functions:dev": "cross-env FUNCTIONS_WATCH=true tsx watch scripts/build-functions.ts",
    "dev:netlify": "npm run functions:build && concurrently \"npm run functions:dev\" \"netlify dev --port 8888\""
